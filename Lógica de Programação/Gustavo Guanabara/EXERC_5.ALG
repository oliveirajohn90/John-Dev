algoritmo "exerc_5"
var
   Nota1, Nota2, Media : real

inicio

      escreval ("Informe a nota da matéria 1: ")
      leia (Nota1)
      escreval ("Informe a nota da Materia 2: ")
      leia (Nota2)
      Media<- (Nota1+Nota2)/2
      escreval ("A média das matérias é de: ", Media)
      
fimalgoritmo