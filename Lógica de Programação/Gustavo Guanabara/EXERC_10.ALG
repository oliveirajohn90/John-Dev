algoritmo "exerc_10"
var

   altura, largura, A : real
   l : real

inicio

      escreval ("Informe a largura da parede:")
      leia (largura)
      escreval ("Informe a altura da parede:")
      leia (altura)
      A<-largura * altura
      escreval ("A area toral da parede é de:", A)
      l<-(A*1/2)
      escreval ("A quantidade de litros para pintar a parede será de: ", l)

fimalgoritmo